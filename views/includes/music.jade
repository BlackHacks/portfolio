script(type='text/x-handlebars', data-template-name='Music')
  {{view App.MusicMenuView}}

script(type='text/x-handlebars', data-template-name='music-menu')
  .navbar.navbar-fixed-top
    nav
      ul.clearfix
        <li><a href="#" {{action gotoHome}}>Home</a></li>
        li
          a.nav-blog(href="http://blog.piotry.me") Blog
        <li class="active"><a href="#" {{action gotoMusic}} >Music</a></li>
        <li><a href="#" {{action gotoPortfolio}} >Portfolio</a></li>
        <li><a href="#" {{action gotoAbout}} >About</a></li>

  .container
    header#overview.jumbotron.head
      h1 Music I produce

  .container.show-grid
    ul.nav.nav-tabs
      <li><a href="#" {{action gotoSoundCloud}}>Soundcloud</a></li>
      <li><a href="#" {{action gotoYoutube}}>Youtube</a></li>
      li
        a.nav-about(href='/music') Timeline

